<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from wsalumnosService WSDL service -->

  <service name="wsalumnos" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ws.oneseven.com"/>
      <parameter name="wsdlServiceElement" value="wsalumnosService"/>
      <parameter name="schemaQualified" value="http://ws.oneseven.com,http://entities.oneseven.com"/>
      <parameter name="wsdlServicePort" value="wsalumnos"/>
      <parameter name="className" value="com.oneseven.ws.WsalumnosSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="wsalumnos"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="searchStudent" qname="operNS:searchStudent" xmlns:operNS="http://ws.oneseven.com" returnQName="retNS:searchStudentReturn" xmlns:retNS="http://ws.oneseven.com" returnType="rtns:Student" xmlns:rtns="http://entities.oneseven.com" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://ws.oneseven.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="loginStudent" qname="operNS:loginStudent" xmlns:operNS="http://ws.oneseven.com" returnQName="retNS:loginStudentReturn" xmlns:retNS="http://ws.oneseven.com" returnType="rtns:Student" xmlns:rtns="http://entities.oneseven.com" soapAction="" >
        <parameter qname="pns:user" xmlns:pns="http://ws.oneseven.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:pass" xmlns:pns="http://ws.oneseven.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="loginStudent searchStudent"/>

      <typeMapping
        xmlns:ns="http://entities.oneseven.com"
        qname="ns:Student"
        type="java:com.oneseven.entities.Student"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
